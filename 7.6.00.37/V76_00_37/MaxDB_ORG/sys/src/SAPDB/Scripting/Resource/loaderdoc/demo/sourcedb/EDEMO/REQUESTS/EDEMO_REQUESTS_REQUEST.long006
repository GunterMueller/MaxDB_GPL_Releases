SELECT C."Category Name", P."Product Name", "Product Sales" = Sum(OD."Unit Price" * OD."Quantity" * (1 - OD."Discount") * 100 / 100) FROM "Categories" C, "Products" P, "Order Details" OD, "Orders" O WHERE C."Category ID" = P."Category ID" AND P."Product ID" = OD."Product ID" AND OD."Order ID" = O."Order ID" AND O."Order Date" BETWEEN '1993-01-01' AND '1993-12-31' GROUP BY C."Category Name", P."Product Name"