ERRORMSG LONG !
CREATE TRIGGER T FOR T1 AFTER INSERT EXECUTE (
  VAR I INTEGER;
      C CHAR(20);
      RET FIXED(5);
  SET I = S1 + S2 + 101;
  SET S1 = S1 + 1;
  SET S2 = I;
  INSERT KERN.T2 VALUES (:S1, :S2);
  IF $RC = 0
  THEN
      DECLARE RES CURSOR FOR
        SELECT * FROM KERN.T2 WHERE S1 = :S1 AND S2 = :S2;
  SET I = 0;
  WHILE $RC = 0 DO
    BEGIN
    FETCH RES INTO :S1, :I;
    IF ($RC = 0) AND (I <> S2)
    THEN
      STOP (-1000, 'i <> s2');
    END;
  IF $RC = 100
  THEN
    SET RET = 0
  ELSE
    SET RET = $RC;
  CLOSE RES;
  IF RET <> 0
  THEN
    STOP(RET);
) !
 
