<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE statements SYSTEM "sequencedescription.dtd">
<statements>

	<execute statement="create schema [USER]S2"/>
	<execute statement="commit"/>

	<execute statement="inner transaction begin"/>
	<execute statement="rename schema [USER]S2 to [USER]S3"/>
	<execute statement="inner transaction commit"/>

	<execute statement="create table [USER]S2.TAB (ID integer primary key, NAME char(20))" error="-4030"/>
	<execute statement="insert [USER]S2.TAB values (1,'n1')" error="-4004"/>
	<execute statement="select * from [USER]S2.TAB" error="-4004"/>

	<execute statement="create table [USER]S3.TAB (ID integer primary key, NAME char(20))"/>
	<execute statement="insert [USER]S3.TAB values (1,'n1')"/>
	<execute statement="insert [USER]S3.TAB values (2,'n2')"/>
	<check select="select * from [USER]S3.TAB" result="([ID:integer, NAME:char]), (1,'n1'), (2,'n2')"/>
	
	<execute statement="commit"/>

</statements>