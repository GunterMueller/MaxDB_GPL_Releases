SQLMODE INTERNAL
//
USE SCHEMA "T19SCHEMA"
//
USE SCHEMA "T19SCHEMA"
//
CREATE TABLE "T19TABLE1"
(
	"TID"  Integer  NOT NULL,
	"TNAME"  Char (30) ASCII,
	"TVORNAME"  Char (30) ASCII,
	PRIMARY KEY ("TID")
)
//
CREATE TABLE "T19TABLE2"
(
	"TNO"  Integer  NOT NULL,
	"TTITEL"  Char (30) ASCII,
	"TAUTOR"  Integer,
	PRIMARY KEY ("TNO")
)
//
USE SCHEMA "T19SCHEMA"
//
USE SCHEMA "T19SCHEMA"
//
ALTER TABLE "T19TABLE2" FOREIGN KEY "F_TABLE2" ("TAUTOR") REFERENCES "T19TABLE1" ("TID") ON DELETE  RESTRICT
//
USE SCHEMA "T19SCHEMA"
//
CREATE VIEW T19SCHEMA.T19VIEW AS SELECT TNO,TTITEL,TNAME FROM T19SCHEMA.T19TABLE1, T19SCHEMA.T19TABLE2
WHERE T19TABLE2.TAUTOR = T19TABLE1.TID

//
GRANT SELECT ON T19SCHEMA.T19VIEW TO T19ZWEI
