<Test ErrorType="syntax" Exclude="" Number="1" Responsible="Martin Ki" TestType="negative">
<Comment>
    PTS 1105795: implementation of INSERT/SELECT for LONG
                 columns
    Test 1: negatives; check that INSERT/SELECT fails for incompatible
            LONG types
</Comment>
<Prepare>
<Statement callable="false" ignore="true" verify="false">
<CallString>
      DROP TABLE CT1
    </CallString>
</Statement>
<Statement callable="false" ignore="true" verify="false">
<CallString>
      DROP TABLE CT3
    </CallString>
</Statement>
<Statement callable="false" ignore="false" verify="false">
<CallString>
      CREATE TABLE CT1 (T1C1 CHAR(25), T1C2 LONG ASCII)
    </CallString>
</Statement>
<Statement callable="false" ignore="false" verify="false">
<CallString>
      CREATE TABLE CT3 (T3C1 CHAR(25), T3C2 LONG BYTE)
    </CallString>
</Statement>
<Statement callable="false" ignore="false" verify="false">
<CallString>
      INSERT INTO CT1 VALUES ('r1', 'long text')
    </CallString>
</Statement>
<Statement callable="false" ignore="false" verify="false">
<CallString>
      INSERT INTO CT1 VALUES ('r2', 'more text')
    </CallString>
</Statement>
<Statement callable="false" ignore="false" verify="false">
<CallString>
      INSERT INTO CT3 VALUES ('r1', 'long text')</CallString>
</Statement>
</Prepare>
<Run>
<Statement callable="false" ignore="false" verify="false">
<CallString>
INSERT INTO CT1 SELECT * FROM CT3</CallString>
<Error>
<ErrorCode>-8006</ErrorCode>
<ErrorMessage>[-8006]: Data types must be compatible:T1C2</ErrorMessage>
</Error>
</Statement>
<Statement callable="false" ignore="false" verify="false">
<CallString>
      INSERT INTO CT3 SELECT * FROM CT1
    </CallString>
<Error>
<ErrorCode>-8006</ErrorCode>
<ErrorMessage>Data types must be compatible:T3C2</ErrorMessage>
</Error>
</Statement>
</Run>
<CleanUp>
<Statement callable="false" ignore="false" verify="false">
<CallString>
      DROP TABLE CT1
    </CallString>
</Statement>
<Statement callable="false" ignore="false" verify="false">
<CallString>
      DROP TABLE CT3
    </CallString>
</Statement>
<Statement callable="false" ignore="false" verify="false">
<CallString>
      COMMIT
    </CallString>
</Statement>
</CleanUp>
</Test>
