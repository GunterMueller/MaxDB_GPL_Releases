SQLMODE INTERNAL
//
USE SCHEMA "T16SCHEMA1"
//
USE SCHEMA "T16SCHEMA1"
//
CREATE TABLE "T16TABLE1"
(
	"TID"  Integer  NOT NULL,
	"TNAME"  Char (30) ASCII,
	"TVORNAME"  Char (30) ASCII,
	PRIMARY KEY ("TID")
)
//
CREATE TABLE "T16TABLE2"
(
	"TNO"  Integer  NOT NULL,
	"TTITEL"  Char (30) ASCII,
	"TAUTOR"  Integer,
	PRIMARY KEY ("TNO")
)
//
USE SCHEMA "T16SCHEMA1"
//
USE SCHEMA "T16SCHEMA1"
//
ALTER TABLE "T16TABLE2" FOREIGN KEY "F_TABLE2" ("TAUTOR") REFERENCES "T16TABLE1" ("TID") ON DELETE  RESTRICT
//
USE SCHEMA "T16SCHEMA1"
//
CREATE VIEW T16SCHEMA1.T16VIEW AS SELECT TNO,TTITEL,TNAME FROM T16SCHEMA1.T16TABLE1, T16SCHEMA1.T16TABLE2
WHERE T16TABLE2.TAUTOR = T16TABLE1.TID
