<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE statements SYSTEM "sequencedescription.dtd">
<statements>

	<!-- test -->
	<execute statement="create table TAB (ID integer primary key, NAME char(20))"/>
	<execute statement="insert TAB values(1,'n1')"/>
	<execute statement="grant select on TAB to PETERUWE32"/>
	<execute statement="commit"/>

	<execute statement="inner transaction begin"/>
	<execute statement="revoke select on TAB from PETERUWE32"/>
	<execute statement="inner transaction commit"/>

	<sync/>

	<execute statement="select * from PETERUWE32.TAB" error="-4004"/>
	<execute statement="commit"/>

</statements>