<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE config SYSTEM "config.dtd">
<!--
	Configuration for Windows Systems
	We don't need the unix user stuff in here
  -->
<config>
	<settings>
		<log>
			<level>verbose</level>
			<file autocreate="true"> </file>
		</log>
		<sapcar path="/SAPDBInstall/sarfile/SAPCAR.EXE"/>
		<packdistrib path="/SAPDevelop/Devtool/bin/packdistrib.pl"/>
		<directories>
			<temp>/TEMP/sapdb</temp>
			<base>/sapdb</base>
			<indep_data>/sapdb/indep_data</indep_data>
			<indep_prog>/sapdb/indep_prog</indep_prog>
			<depend>/sapdb/depend</depend>
		</directories>
		<dbmcli command="dbmcli"/>
	</settings>
</config>