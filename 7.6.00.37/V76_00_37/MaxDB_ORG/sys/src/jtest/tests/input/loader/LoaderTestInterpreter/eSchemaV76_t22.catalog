CREATE TABLE "T22TABLE"
(
	"TID"  Integer  NOT NULL,
	"TNAME"  Char (30) ASCII,
	PRIMARY KEY ("TID")
)
//
CREATE TRIGGER T22TRIGGER FOR T22SCHEMA1.T22TABLE
AFTER DELETE EXECUTE
(  INSERT INTO T22SCHEMA1.T22SAVETABLE 
  SET SID=:OLD.TID, SNAME=:OLD. TNAME, SUSER=USER, SDATUM=DATE;
)
