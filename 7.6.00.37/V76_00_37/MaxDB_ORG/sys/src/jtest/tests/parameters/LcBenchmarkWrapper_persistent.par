# parameter file for LcBenchmarkWrapper

TraceLevel=1

# test database name
TestDBName=BENCH76

# test database action before test
BeforeDBAction=FORCE

# test database action after test
AfterDBAction=KEEP

# -mini (250MB), -small (550MB), -medium (1,6GB), -large  (3,2GB) OR -huge   ( 18GB)
SizeOption=-large

StoreResultPersistent=true

# -multi OR -single
UserSetting=-multi
        
# info on the results database
ResultsDBName=QADB
#ResultsDBServer=10.21.81.184:7210
ResultsDBServer=PTS:7210
ResultsDBUser=BENCHMARK
ResultsDBPassword=BENCHMARK
