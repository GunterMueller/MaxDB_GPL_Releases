# parameter file for test SutStarter

# kernel version (fast, quick, slow)
# default: slow
# kernel=slow

# code type (dbuni, uni, xci, alluni)
# default: none
# deactivate fpr ASCII tests
# codetype=alluni

# zip file name for upload
# default: sutprt.zip
zipfile=<MAJOR_VERSION><MINOR_VERSION><FORMATED_CORRECTION_LEVEL><LC_STATE>_sutprt.zip.sck

# throw errors instead of warnings for SUT differences (true, false)
# default: false
diffErr=true

# this string contains the release and the qa level 
release=<MAJOR_VERSION><MINOR_VERSION><FORMATED_CORRECTION_LEVEL><LC_STATE>

# if patchISUTFile is set to true the test changes the ISUT file according to codtype
patchISUTFile=true
