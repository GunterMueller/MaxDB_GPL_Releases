# parameter file for WebDAV java tests

# host name of WebDAV test server
# default: localhost
host=localhost

# port of WebDAV test server
# default: 85
port=1085

# baseUrl used for WebDAV tests
# default: /webdav
baseUrl=/webdav

# overwrite-flag for COPY-operations
# default: F
var_overwrite=F

# locktoken
# default: null
# locktoken=null

# boolean flag to decide whether the webdav server shall be started by each test itsself
# default: false
start_server=true