# parameter file for LcBenchmarkWrapper

TraceLevel=1

# test database name
TestDBName=BENCH76

# test database action before test
BeforeDBAction=FORCE

# test database action after test
AfterDBAction=KEEP

# -mini (250MB), -small (550MB), -medium (1,6GB), -large  (3,2GB) OR -huge   ( 18GB)
SizeOption=-small

StoreResultPersistent=false

# -multi OR -single
UserSetting=-multi
        
