include	../Makedefs

.SUFFIXES :	.y .l .c .h .o .a
.y.c :
.l.c :
.c.obj :
	$(CC) $(CFLAGS) -c $<
.c.lib :
.mf.p :
.p.c :
.p.o :


all :		mfcline
install :	$(TOOL)/pgm/mfcline
clean :
	-$(RM) *.o *.prt prot mfcline

mfcline :			mfcline.o
	$(LD) $(LFLAGS) -o mfcline  mfcline.o
$(TOOL)/pgm/mfcline :	mfcline.o
	$(LD) $(LFLAGS) -o mfcline  mfcline.o
	-$(RM) $(TOOL)/pgm/mfcline
	cp mfcline $(TOOL)/pgm

