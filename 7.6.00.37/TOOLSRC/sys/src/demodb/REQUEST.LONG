SELECT O."Order Date", "Count of Order ID" = Count(O."Order ID"), "Sum of Subtotals" = Sum(OS."Subtotal") FROM "Orders" O, "Order Subtotals" OS WHERE O."Order ID" = OS."Order ID" GROUP BY O."Order Date"SELECT "Order ID", "Subtotals" = Sum("Unit Price" * "Quantity" * (1 - "Discount") * 100 / 100) FROM "Order Details" GROUP BY "Order ID"SELECT C."Category Name", P."Product Name", P."Product ID", P."Units In Stock", P."Reorder Level", P."Units On Order", P."Quantity Per Unit", P."Unit Price", C."Description", P."Discontinued" FROM "Categories" C, "Products" P WHERE C."Category ID" = P."Category ID" AND P."Discontinued" = FALSE ORDER BY C."Category Name", P."Product Name"SELECT P."Product Name", P."Product ID", P."Category ID", P."Supplier ID", P."Quantity Per Unit", P."Unit Price", P."Units In Stock", P."Units On Order", P."Reorder Level", P."Discontinued", S."Company Name", S."Contact Name", S."City", S."Country", S."Phone" FROM "Products" P, "Suppliers" S WHERE S."Supplier ID" = P."Supplier ID" ORDER BY P."Product Name"ÉSELECT C."Category Name", P."Product Name", P."Units In Stock", P."Units On Order", S."Company Name", S."Phone" FROM "Categories" C, "Products" P, "Suppliers" S WHERE C."Category ID" = P."Category ID" AND S."Supplier ID" = P."Supplier ID"SELECT C."Category Name", P."Product Name", "Product Sales" = Sum(OD."Unit Price" * OD."Quantity" * (1 - OD."Discount") * 100 / 100) FROM "Categories" C, "Products" P, "Order Details" OD, "Orders" O WHERE C."Category ID" = P."Category ID" AND P."Product ID" = OD."Product ID" AND OD."Order ID" = O."Order ID" AND O."Order Date" BETWEEN '1993-01-01' AND '1993-12-31' GROUP BY C."Category Name", P."Product Name"