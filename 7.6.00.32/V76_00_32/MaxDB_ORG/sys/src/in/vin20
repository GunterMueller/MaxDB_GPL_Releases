(*------------------------------*) 
 
PROCEDURE
      i20upcmnd (
            VAR s  : tsp00_Buf;
            l_s    : tin_natural);
 
VAR
      i    : integer;
      j    : integer;
      help : tsp00_Buf;
 
BEGIN
IF  i01g^.multibyte.dblang = bsp_c64
THEN
    i21pcmnd (i01g, s, l_s)
ELSE
    BEGIN
    i := 1;
    j := 2;
    WHILE i <= l_s DO
        BEGIN
        IF  i01g^.multibyte.unicode_type = csp_unicode
        THEN
            BEGIN
            help [j - 1] := csp_unicode_mark;
            help [j]     := s [i];
            END
        ELSE
            BEGIN
            help [j - 1] := s [i];
            help [j]     := csp_unicode_mark;
            END;
        (*ENDIF*) 
        i := i + 1;
        IF  j = mxsp_buf
        THEN
            BEGIN
            i21pcmnd (i01g, help, mxsp_buf);
            j := 2;
            END
        ELSE
            j := j + 2;
        (*ENDIF*) 
        END;
    (*ENDWHILE*) 
    i21pcmnd (i01g, help, j - 2)
    END;
(*ENDIF*) 
END; (* i20upcmnd *)
 
(*------------------------------*) 
 
