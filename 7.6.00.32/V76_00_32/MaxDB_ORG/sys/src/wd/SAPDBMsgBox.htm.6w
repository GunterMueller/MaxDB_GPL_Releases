<html>
  <head>
    <meta http-equiv="Expires" content="Sat, 01 Jan 1990 00:00:00 GMT">
    <link rel="stylesheet" href="/WARoot/Styles/sapdbweb.css" type="text/css">
    <script language="JavaScript" src="/WARoot/JavaScripts/wqbutton.js"></script>
    <title>SAP DB Web Access</title>
  </head>
  <body topmargin="0" leftmargin="0" marginwidth="0" marginheight="0" class="BdyStd">
    <!-- TemplateKey Logo -->
    <!-- EndTemplateKey Logo -->
    <table border="0" cellpadding="0" cellspacing="0">
        <tr>
            <td><img src="/WARoot/Images/1x1.gif" height="20" width=20></td>
        </tr>
        <tr>
            <td><img src="/WARoot/Images/1x1.gif"></td>
            <td>
                <table border="0" cellpadding="0" cellspacing="0" class="GrpWhlBox">
                    <tr>
                        <!-- TemplateKey Title -->
                        <td class="GrpTtlBox"><nobr>%Title%</nobr></td>
                        <!-- EndTemplateKey Title -->
                    </tr>
                    <tr>
                        <td valign="top" class="GrpBdyBox">
                            <table border="0" cellpadding="0" cellspacing="0">
                                <tr>
                                    <td>
                                        <!-- TemplateKey Message -->
                                        <div class="MsgBarStd">
                                            <table border="0" cellpadding="0" cellspacing="0">
                                                <tr>
                                                    <td class="MsgBarImgOk"><img src="/WARoot/Images/1x1.gif">
                                                    </td>
                                                    <td>
                                                        <span class="TxtStd">%Message%</span>
                                                    </td>
                                                </tr>
                                            </table>
                                        </div>
                                        <!-- EndTemplateKey Message -->
                                        <!-- TemplateKey MessageList -->
                                        <div class="MsgBarErr">
                                            <table border="0" cellpadding="0" cellspacing="0">
                                                <tr>
                                                    <td class="MsgBarImgError"><img src="/WARoot/Images/1x1.gif">
                                                    </td>
                                                    <td>
                                                        <table border="0" cellpadding="0" cellspacing="0">
                                                            <tr>
                                                                <td>
                                                                    <!-- TemplateKey MessageNumber -->
                                                                    <span class="TxtStd">%MessageNumber%</span>
                                                                    <!-- EndTemplateKey MessageNumber -->
                                                                    <!-- TemplateKey MessageText -->
                                                                    <span class="TxtStd">%MessageText%</span>
                                                                    <!-- EndTemplateKey MessageText -->
                                                                </td>
                                                            </tr>
                                                        </table>
                                                    </td>
                                                </tr>
                                            </table>
                                        </div>
                                        <!-- EndTemplateKey MessageList -->
                                    </td>
                                </tr>
                                <!-- Separator -->
                                <tr>
                                    <td><img src="/WARoot/Images/1x1.gif" height="10"></td>
                                </tr>
                                <!-- Buttons -->
                                <tr>
                                    <td>
                                        <!-- TemplateKey Button -->
                                        <script>ButtonLink(%Button%)</script>
                                        <!-- EndTemplateKey Button -->
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>
  </body>
</html>
